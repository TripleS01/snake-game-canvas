{
    "env": {
        "es2021": true,
        "browser": true
    },
    "globals": {},
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": "error",
        "curly": "error",
        "quotes": [
            "warn",
            "single"
        ],
        "no-undef": "error",
        "indent":[
            "error",
            4
        ]
    }
}